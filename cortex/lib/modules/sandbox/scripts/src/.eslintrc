{
  "parser": "babel-eslint",
  "globals": {
    "consts": true,
    "org": true,
    "script": true,
    "sys": true,
    "CortexObject": true,
    "ObjectID": true,
    "Fault": true
  },
  "rules": {
    "import/no-unresolved": 0,
    "camelcase": ["error", {"allow":  ["c_*"]}],
    "no-underscore-dangle": ["error", { "allow": ["_id", "__post"], "allowAfterThis": true }],
    "no-console": "off"

  }
}